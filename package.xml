<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project gnx-tools">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
	<!-- javac -d bin/ src/uk/ac/ebi/gnx/*  -->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="gnx.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="uk.ac.ebi.gnx.CalculateGNx"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="bin"/>
        </jar>
    </target>
</project>
